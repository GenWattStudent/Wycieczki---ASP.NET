@{
    ViewBag.Title = "Active Tours";
}

@model List<TourModel>

<header class="glassmorphism p-3 rounded">
    <h2>Active Tours</h2>
</header>

<ul class="grid-tour gap-3 p-0 mt-3 justify-content-center">
    @foreach (var tour in Model)
    {
        <partial name="_TourPartial" model="tour" />
    }
</ul>
